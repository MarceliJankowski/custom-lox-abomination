# integer subtraction and non-commutativity
print 4 - 3; #ASSERT_STDOUT_LINE 1
print 3 - 4; #ASSERT_STDOUT_LINE -1

# subtraction properties
print 2 - 0; #ASSERT_STDOUT_LINE 2
print 25 - 25; #ASSERT_STDOUT_LINE 0

# floating-point subtraction
print 3.75 - 2.45; #ASSERT_STDOUT_LINE 1.3

# signed integer subtraction
print -4 - 3; #ASSERT_STDOUT_LINE -7
print 4 - -3; #ASSERT_STDOUT_LINE 7
print -4 - -3; #ASSERT_STDOUT_LINE -1

# signed zero subtraction
print 0 - 0; #ASSERT_STDOUT_LINE 0
print 0 - -0; #ASSERT_STDOUT_LINE 0
print -0 - 0; #ASSERT_STDOUT_LINE -0
print -0 - -0; #ASSERT_STDOUT_LINE 0
